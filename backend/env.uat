# UAT Environment Configuration
ENVIRONMENT=uat
DEBUG=false
LOG_LEVEL=INFO

# Database Configuration
UAT_DB_HOST=uat-db.enwhe.io
UAT_DB_PORT=5432
UAT_DB_NAME=conversationalcommerce_uat
UAT_DB_USER=uat_user
UAT_DB_PASSWORD=your-uat-db-password

# Redis Configuration
UAT_REDIS_HOST=uat-redis.enwhe.io
UAT_REDIS_PORT=6379
UAT_REDIS_PASSWORD=your-uat-redis-password

# Security Keys
UAT_SECRET_KEY=your-uat-secret-key
UAT_JWT_SECRET=your-uat-jwt-secret

# External Services
UAT_CLERK_SECRET_KEY=your-uat-clerk-secret
UAT_CLOUDINARY_URL=your-uat-cloudinary-url
UAT_TWILIO_ACCOUNT_SID=your-uat-twilio-sid
UAT_TWILIO_AUTH_TOKEN=your-uat-twilio-token

# Monitoring
UAT_SENTRY_DSN=your-uat-sentry-dsn

# Rate Limiting
RATE_LIMIT_REQUESTS=500
RATE_LIMIT_WINDOW=60